[36m@@ -0,0 +1,217 @@[m
[32m+[m[32m// SPDX-License-Identifier: MIT[m
[32m+[m[32mpragma solidity ^0.8.19;[m
[32m+[m
[32m+[m[32mcontract MicroLoanPool {[m
[32m+[m[32m    struct Student {[m
[32m+[m[32m        string name;[m
[32m+[m[32m        string university;[m
[32m+[m[32m        uint256 academicScore;[m
[32m+[m[32m        uint256 totalBorrowed;[m
[32m+[m[32m        uint256 totalRepaid;[m
[32m+[m[32m        bool isActive;[m
[32m+[m[32m        uint256 creditScore;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    struct Loan {[m
[32m+[m[32m        uint256 amount;[m
[32m+[m[32m        uint256 interestRate;[m
[32m+[m[32m        uint256 dueDate;[m
[32m+[m[32m        bool isRepaid;[m
[32m+[m[32m        uint256 repaidAmount;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    mapping(address => Student) public students;[m
[32m+[m[32m    mapping(address => Loan[]) public studentLoans;[m
[32m+[m[32m    mapping(address => uint256) public deposits; // Para inversores[m
[32m+[m[41m    [m
[32m+[m[32m    address[] public studentAddresses;[m
[32m+[m[32m    address[] public investors;[m
[32m+[m[41m    [m
[32m+[m[32m    uint256 public totalPoolBalance;[m
[32m+[m[32m    uint256 public totalLoansIssued;[m
[32m+[m[41m    [m
[32m+[m[32m    event StudentRegistered(address indexed student, string name, uint256 academicScore);[m
[32m+[m[32m    event LoanIssued(address indexed student, uint256 amount, uint256 interestRate);[m
[32m+[m[32m    event LoanRepaid(address indexed student, uint256 amount);[m
[32m+[m[32m    event DepositMade(address indexed investor, uint256 amount);[m
[32m+[m[41m    [m
[32m+[m[32m    // Registro de estudiantes[m
[32m+[m[32m    function registerStudent([m
[32m+[m[32m        string memory _name,[m
[32m+[m[32m        string memory _university,[m
[32m+[m[32m        uint256 _academicScore[m
[32m+[m[32m    ) external {[m
[32m+[m[32m        require(_academicScore >= 0 && _academicScore <= 100, "Invalid academic score");[m
[32m+[m[32m        require(!students[msg.sender].isActive, "Student already registered");[m
[32m+[m[41m        [m
[32m+[m[32m        students[msg.sender] = Student({[m
[32m+[m[32m            name: _name,[m
[32m+[m[32m            university: _university,[m
[32m+[m[32m            academicScore: _academicScore,[m
[32m+[m[32m            totalBorrowed: 0,[m
[32m+[m[32m            totalRepaid: 0,[m
[32m+[m[32m            isActive: true,[m
[32m+[m[32m            creditScore: calculateInitialCreditScore(_academicScore)[m
[32m+[m[32m        });[m
[32m+[m[41m        [m
[32m+[m[32m        studentAddresses.push(msg.sender);[m
[32m+[m[32m        emit StudentRegistered(msg.sender, _name, _academicScore);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Calcular score crediticio inicial basado en rendimiento acad√©mico[m
[32m+[m[32m    function calculateInitialCreditScore(uint256 _academicScore) internal pure returns (uint256) {[m
[32m+[m[32m        if (_academicScore >= 90) return 850;[m
[32m+[m[32m        if (_academicScore >= 80) return 750;[m
[32m+[m[32m        if (_academicScore >= 70) return 650;[m
[32m+[m[32m        if (_academicScore >= 60) return 550;[m
[32m+[m[32m        return 450;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Inversores depositan fondos[m
[32m+[m[32m    function deposit() external payable {[m
[32m+[m[32m        require(msg.value > 0, "Deposit must be greater than 0");[m
[32m+[m[41m        [m
[32m+[m[32m        if (deposits[msg.sender] == 0) {[m
[32m+[m[32m            investors.push(msg.sender);[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        deposits[msg.sender] += msg.value;[m
[32m+[m[32m        totalPoolBalance += msg.value;[m
[32m+[m[41m        [m
[32m+[m[32m        emit DepositMade(msg.sender, msg.value);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Estudiantes solicitan pr√©stamos[m
[32m+[m[32m    function requestLoan(uint256 _amount) external {[m
[32m+[m[32m        require(students[msg.sender].isActive, "Student not registered");[m
[32m+[m[32m        require(_amount > 0, "Loan amount must be greater than 0");[m
[32m+[m[32m        require(_amount <= getMaxLoanAmount(msg.sender), "Requested amount exceeds credit limit");[m
[32m+[m[32m        require(totalPoolBalance >= _amount, "Insufficient pool balance");[m
[32m+[m[41m        [m
[32m+[m[32m        uint256 interestRate = calculateInterestRate(msg.sender);[m
[32m+[m[32m        uint256 dueDate = block.timestamp + 180 days; // 6 meses para pagar[m
[32m+[m[41m        [m
[32m+[m[32m        studentLoans[msg.sender].push(Loan({[m
[32m+[m[32m            amount: _amount,[m
[32m+[m[32m            interestRate: interestRate,[m
[32m+[m[32m            dueDate: dueDate,[m
[32m+[m[32m            isRepaid: false,[m
[32m+[m[32m            repaidAmount: 0[m
[32m+[m[32m        }));[m
[32m+[m[41m        [m
[32m+[m[32m        students[msg.sender].totalBorrowed += _amount;[m
[32m+[m[32m        totalPoolBalance -= _amount;[m
[32m+[m[32m        totalLoansIssued += _amount;[m
[32m+[m[41m        [m
[32m+[m[32m        // Transferir fondos al estudiante[m
[32m+[m[32m        payable(msg.sender).transfer(_amount);[m
[32m+[m[41m        [m
[32m+[m[32m        emit LoanIssued(msg.sender, _amount, interestRate);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Calcular monto m√°ximo de pr√©stamo basado en credit score[m
[32m+[m[32m    function getMaxLoanAmount(address _student) public view returns (uint256) {[m
[32m+[m[32m        uint256 creditScore = students[_student].creditScore;[m
[32m+[m[41m        [m
[32m+[m[32m        if (creditScore >= 800) return 1 ether;[m
[32m+[m[32m        if (creditScore >= 700) return 0.5 ether;[m
[32m+[m[32m        if (creditScore >= 600) return 0.3 ether;[m
[32m+[m[32m        return 0.1 ether;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Calcular tasa de inter√©s basada en credit score[m
[32m+[m[32m    function calculateInterestRate(address _student) internal view returns (uint256) {[m
[32m+[m[32m        uint256 creditScore = students[_student].creditScore;[m
[32m+[m[41m        [m
[32m+[m[32m        if (creditScore >= 800) return 5;  // 5% anual[m
[32m+[m[32m        if (creditScore >= 700) return 8;  // 8% anual[m
[32m+[m[32m        if (creditScore >= 600) return 12; // 12% anual[m
[32m+[m[32m        return 15; // 15% anual[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Repagar pr√©stamo[m
[32m+[m[32m    function repayLoan(uint256 _loanIndex) external payable {[m
[32m+[m[32m        require(_loanIndex < studentLoans[msg.sender].length, "Invalid loan index");[m
[32m+[m[41m        [m
[32m+[m[32m        Loan storage loan = studentLoans[msg.sender][_loanIndex];[m
[32m+[m[32m        require(!loan.isRepaid, "Loan already repaid");[m
[32m+[m[32m        require(msg.value > 0, "Payment must be greater than 0");[m
[32m+[m[41m        [m
[32m+[m[32m        loan.repaidAmount += msg.value;[m
[32m+[m[32m        students[msg.sender].totalRepaid += msg.value;[m
[32m+[m[32m        totalPoolBalance += msg.value;[m
[32m+[m[41m        [m
[32m+[m[32m        // Calcular total adeudado (capital + inter√©s)[m
[32m+[m[32m        uint256 totalOwed = loan.amount + (loan.amount * loan.interestRate / 100);[m
[32m+[m[41m        [m
[32m+[m[32m        if (loan.repaidAmount >= totalOwed) {[m
[32m+[m[32m            loan.isRepaid = true;[m
[32m+[m[32m            // Mejorar credit score por pago completo[m
[32m+[m[32m            if (block.timestamp <= loan.dueDate) {[m
[32m+[m[32m                students[msg.sender].creditScore += 10; // Bonus por pago a tiempo[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        emit LoanRepaid(msg.sender, msg.value);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Obtener informaci√≥n del estudiante[m
[32m+[m[32m    function getStudentInfo(address _student) external view returns ([m
[32m+[m[32m        string memory name,[m
[32m+[m[32m        string memory university,[m
[32m+[m[32m        uint256 academicScore,[m
[32m+[m[32m        uint256 totalBorrowed,[m
[32m+[m[32m        uint256 totalRepaid,[m
[32m+[m[32m        uint256 creditScore,[m
[32m+[m[32m        bool isActive[m
[32m+[m[32m    ) {[m
[32m+[m[32m        Student memory student = students[_student];[m
[32m+[m[32m        return ([m
[32m+[m[32m            student.name,[m
[32m+[m[32m            student.university,[m
[32m+[m[32m            student.academicScore,[m
[32m+[m[32m            student.totalBorrowed,[m
[32m+[m[32m            student.totalRepaid,[m
[32m+[m[32m            student.creditScore,[m
[32m+[m[32m            student.isActive[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Obtener pr√©stamos del estudiante[m
[32m+[m[32m    function getStudentLoans(address _student) external view returns ([m
[32m+[m[32m        uint256[] memory amounts,[m
[32m+[m[32m        uint256[] memory interestRates,[m
[32m+[m[32m        uint256[] memory dueDates,[m
[32m+[m[32m        bool[] memory repaidStatus[m
[32m+[m[32m    ) {[m
[32m+[m[32m        Loan[] memory loans = studentLoans[_student];[m
[32m+[m[32m        uint256 length = loans.length;[m
[32m+[m[41m        [m
[32m+[m[32m        amounts = new uint256[](length);[m
[32m+[m[32m        interestRates = new uint256[](length);[m
[32m+[m[32m        dueDates = new uint256[](length);[m
[32m+[m[32m        repaidStatus = new bool[](length);[m
[32m+[m[41m        [m
[32m+[m[32m        for (uint256 i = 0; i < length; i++) {[m
[32m+[m[32m            amounts[i] = loans[i].amount;[m
[32m+[m[32m            interestRates[i] = loans[i].interestRate;[m
[32m+[m[32m            dueDates[i] = loans[i].dueDate;[m
[32m+[m[32m            repaidStatus[i] = loans[i].isRepaid;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Estad√≠sticas del pool[m
[32m+[m[32m    function getPoolStats() external view returns ([m
[32m+[m[32m        uint256 totalBalance,[m
[32m+[m[32m        uint256 totalIssued,[m
[32m+[m[32m        uint256 totalStudents,[m
[32m+[m[32m        uint256 totalInvestors[m
[32m+[m[32m    ) {[m
[32m+[m[32m        return ([m
[32m+[m[32m            totalPoolBalance,[m
[32m+[m[32m            totalLoansIssued,[m
[32m+[m[32m            studentAddresses.length,[m
[32m+[m[32m            investors.length[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
