[32m+[m[32mimport { HardhatRuntimeEnvironment } from "hardhat/types";[m
[32m+[m[32mimport { DeployFunction } from "hardhat-deploy/types";[m
[32m+[m
[32m+[m[32mconst deployStudentRegistry: DeployFunction = async function (hre: HardhatRuntimeEnvironment) {[m
[32m+[m[32m  const { deployer } = await hre.getNamedAccounts();[m
[32m+[m[32m  const { deploy } = hre.deployments;[m
[32m+[m
[32m+[m[32m  await deploy("StudentRegistry", {[m
[32m+[m[32m    from: deployer,[m
[32m+[m[32m    log: true,[m
[32m+[m[32m    autoMine: true,[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[32m+[m
