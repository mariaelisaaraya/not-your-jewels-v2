[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    function getStudentCount() external view returns (uint256) {[m
[32m+[m[32m        return studentAddresses.length;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
